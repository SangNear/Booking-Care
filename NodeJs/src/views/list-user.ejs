<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
    <title>GET CRUD</title>
</head>
<style>
    table,
    th,
    td {
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>

<body>
    <h1>GET CRUD FROM VIEW</h1>
    <table style="width: 100%;">
        <tr>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>Action</th>
        </tr>


        <% for(let i=0; i< dataTable.length; i++){ %>

            <tr>
                <th>
                    <%= dataTable[i].email %>
                </th>
                <th>
                    <%= dataTable[i].firstName %>
                </th>
                <th>
                    <%= dataTable[i].lastName %>
                </th>
                <th>
                    <%= dataTable[i].address %>
                </th>
                <th>
                    <a href="/edit-crud?id=<%= dataTable[i].id %>" class="btn btn-warning" type="button">Edit</a>
                    <a href="/delete-crud?id=<%= dataTable[i].id %>" class="btn btn-danger" type="button">Delete</a>

                </th>
            </tr>
            <% } %>
    </table>
</body>

</html>